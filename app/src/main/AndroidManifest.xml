<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abdallah">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.abdallah.AndroidRestaurant.WelcomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.abdallah.AndroidRestaurant.MyRestaurants" />
        <activity android:name="com.example.abdallah.AndroidRestaurant.MainMenu" />
        <activity
            android:name="com.example.abdallah.AndroidRestaurant.AboutUs"
            android:label="@string/title_activity_about_us" />
        <activity
            android:name="com.example.abdallah.AndroidRestaurant.AddRestaurant"
            android:label="@string/title_activity_add_restaurant" />
        <activity
            android:name="com.example.abdallah.AndroidRestaurant.SearchRestaurant"
            android:label="@string/title_activity_search_restaurant"></activity>
    </application>

</manifest>